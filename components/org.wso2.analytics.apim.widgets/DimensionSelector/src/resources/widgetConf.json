{
  "name" : "DIMENSION SELECTOR",
  "id" : "DimensionSelector",
  "thumbnailURL" : "",
  "configs" : {
    "pubsub" : {
      "types": ["publisher"],
      "publisherWidgetOutputs": ["dimension", "selection"]
    },
    "providerConfig" : {
      "configs": {
        "type": "SiddhiStoreDataProvider",
        "config": {
          "siddhiApp": "@App:name('DimensionSelectorSiddhi') @primaryKey('API_ID') @store(type=\"rdbms\" , datasource=\"AM_DB\") define table AM_API(API_ID int,API_PROVIDER string,API_NAME string,API_VERSION string,CONTEXT string,CONTEXT_TEMPLATE string,API_TIER string,API_TYPE string, CREATED_BY string,CREATED_TIME string,UPDATED_BY string,UPDATED_TIME string);",
          "queryData": {
            "apiquery": "from AM_API on {{contextContainsCondition}} select API_NAME,API_VERSION,API_PROVIDER,API_TYPE;"
          },
          "publishingInterval": 360000
        }
      }
    },
    "options": [
      {
        "id" : "header",
        "title" : "Header",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": false
      },
      {
        "id" : "selectMultiple",
        "title" : "Select Multiple",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": true
      },
      {
        "id" : "selectAll",
        "title" : "Select 'All' Option",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": true
      },
      {
        "id" : "dimensions",
        "title" : "Dimension(s)",
        "type" : {
          "name" : "ENUM",
          "multiple" : true,
          "possibleValues" : [
            "API",
            "API Provider"
          ]
        },
        "defaultValue": ["API", "API Provider"]
      },
      {
        "id" : "defaultDimension",
        "title" : "Default Dimension",
        "type" : {
          "name" : "ENUM",
          "possibleValues" : [
            "API",
            "API Provider"
          ]
        },
        "defaultValue": "API"
      }
    ]
  }
}
